@model IEnumerable<RentaCarEntities.Vehicle>


<table id="myTable" class="tablesorter">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(a => a.First()._branchName)</th>
            <th>@Html.DisplayNameFor(a => a.First().Model)</th>
            <th>@Html.DisplayNameFor(a => a.First().FuelType)</th>
            <th>@Html.DisplayNameFor(a => a.First()._brandName)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item._branchName</td>
                <td>@item.Model</td>
                <td>@item.FuelType</td>
                <td>@item._brandName</td>
                <td>
                  
                </td>
            </tr>
        }
    </tbody>

</table>

