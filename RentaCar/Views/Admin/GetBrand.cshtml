@model IEnumerable<RentaCarEntities.Brand>

@{
    ViewBag.Title = "GetBrand";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<table id="myTable" class="tablesorter">
    <thead>
        <tr>

            <th>@Html.DisplayNameFor(a => a.First().Name)</th>
            <th>@Html.DisplayNameFor(a => a.First().Version)</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>@item.Name</td>
                <td>@item.Version</td>


                <td>
                    @Html.ActionLink("Düzenle", "GetEditBrand", new { id = item.Id })
                    @Html.ActionLink("Sil", "DeleteBrand", new { id = item.Id })
                </td>
            </tr>


        }
    </tbody>


</table>
<p>
    @Html.ActionLink("Marka Ekle", "GetCreateBrand")
</p>